@{
    ViewBag.Title = "Upload";
    Layout = Request.IsAjaxRequest() ? null : "~/Views/Shared/_Layout.cshtml";
}

<div class="box box-primary">
    <div class="box-header with-border">
        <h3 class="box-title">Carga masiva de Eventos</h3>
        <div class="box-tools pull-right">
            <button id="btn-CreateVentana" class="btn btn-default" alt="Crear Ventana"><i class="fa fa-edit text-info"></i></button>
        </div>
    </div>
    @using (Html.BeginForm("Upload", "Evento", FormMethod.Post, new { area = "Operaciones", enctype = "multipart/form-data", @class = "form-horizontal" }))
    {
        @Html.AntiForgeryToken()
        <div class="box-body">
            <div class="row">
                <div class="col-md-6">
                    <div class="form-group">
                        <label for="inputEmail3" class="col-sm-2 control-label">Excel</label>
                        <div class="col-md-10">
                            <input type="file" name="file" id="file" />
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="box-footer clearfix">
            <input class="btn btn-sm btn-info btn-flat  pull-right" type="submit" value="Upload" />
        </div>
    }
</div>

@Scripts.Render("~/JS/jquery")
@Scripts.Render("~/JS/Bootstrap")

<script>
    $(function ()
    {
        
    });
</script>

