@model IEnumerable<PMMX.Modelo.Entidades.WorkFlow>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <p>
        @Html.ActionLink("Create New", "Create")
    </p>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Categoria.Nombre)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EstatusAnterior.Nombre)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EstatusCancelado.Nombre)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EstatusInicial.Nombre)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EstatusSiguiente.Nombre)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Activo)
            </th>
            <th></th>
        </tr>
    
    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Categoria.Nombre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EstatusAnterior.Nombre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EstatusCancelado.Nombre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EstatusInicial.Nombre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EstatusSiguiente.Nombre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Activo)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
                @Html.ActionLink("Details", "Details", new { id=item.Id }) |
                @Html.ActionLink("Delete", "Delete", new { id=item.Id })
            </td>
        </tr>
    }
    
    </table>
</body>
</html>
